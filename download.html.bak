                    function getBuildInfos(ver, i) {
                        return new Promise(resolve => {
                            $.getJSON('https://mohistmc.com/api/' + ver + '/' + i, function (build) {
                                resolve(build);
                            });
                        });
                    }

                    class TabManager {
                        constructor() {
                            this.data = [
                                {
                                    version: '1.7.10',
                                    name: '1.7.10 (deprecated)',
                                    main: false,
                                    messages: ["Mohist 1.7.10 is deprecated, its usage is not recommended, and we do not provide support for it.",
                                        `Alternatives: <a href="https://github.com/CrucibleMC/Crucible">CrucibleMC</a>`
                                    ]
                                },
                                {
                                    version: '1.12.2',
                                    name: '1.12.2',
                                    main: false,
                                    messages: [
                                        "Mohist 1.12.2 is now End Of Life. What does it mean? It means that we will not update it as so often as before, and that we will not be able to answer to issues about it. If you want more info about this, please read the Announcement channel in our Discord server.",
                                        `We're currently working with the Magma team to create a better 1.12.2 hybrid software by merging some parts from both software. You can find the repository <a href="https://git.magmafoundation.org/magmafoundation/Magma/-/tree/mohist-merge">there</a>, and all contributions are welcome :D`
                                    ]
                                },
                                {
                                    version: '1.16.5',
                                    name: '1.16.5',
                                    main: true,
                                    messages: []
                                },
                                {
                                    version: '1.19.3',
                                    name: '1.19.3 (Dev)',
                                    main: false,
                                    messages: [
                                        "Important: Mohist 1.19.3 is still experimental and the plugin compatibility is very close to 0%. If you plan to use it for production, please wait for it to be stable.",
                                    ]
                                }
                            ]

                            this.tabs = []
                        }

                        init() {
                            console.log("Initializing TabManager");
                            for (const version_data of this.data.reverse())
                                this.createTab(version_data.version, version_data.name, version_data.main, version_data.messages).insert()
                        }

                        createTab(version, name, main, messages) {
                            const tab = new Tab(version, name, main, messages)
                            this.tabs.push(tab)
                            console.log(`Tab ${version} has been created`);
                            return tab
                        }

                        getTab(version) {
                            return this.tabs.find(tab => tab.getVersion() === version)
                        }
                    }

                    class Tab {
                        constructor(version, name, main, messages) {
                            this.version = version
                            this.name = name
                            this.main = main
                            this.messages = messages

                            this.last_fetched_build = undefined
                            this.last_build_number = undefined

                            //HTML Elements
                            this.build_tab = document.getElementById('build-tab')
                        }

                        async getLastFetchedBuild() {
                            this.last_fetched_build = this.last_fetched_build ?? await this.getLatestBuildNumber()
                            return this.last_fetched_build
                        }

                        async getLastBuildNumber() {
                            this.last_build_number = this.last_build_number ?? await this.getLatestBuildNumber()
                            return this.last_build_number
                        }

                        setLastFetchedBuild(number) {
                            this.last_fetched_build = number
                        }

                        getVersion() {
                            return this.version
                        }

                        getName() {
                            return this.name
                        }

                        isMain() {
                            return this.main
                        }

                        getMessages() {
                            return this.messages
                        }

                        async getLatestBuildNumber() {
                            return new Promise(resolve => {
                                $.getJSON('https://mohistmc.com/api/' + this.getVersion() + '/latest', function (latest) {
                                    resolve(latest.number);
                                });
                            })
                        }

                        insert() {
                            this.build_tab.insertAdjacentHTML('afterbegin', `<li><a class="tab-name-${this.getVersion()}" ${this.isMain() ? 'id="maintab"' : ''}>${this.getName()}</a></li>`)
                            this.build_tab.insertAdjacentHTML('afterend', `<div id="tab-${this.getVersion()}"></div>`)

                            const {tab, tab_name} = this.getHTMLElements()
                            for (const message of this.getMessages())
                                tab.insertAdjacentHTML('beforeend', `<p style="color: #ccc;text-align: center; font-weight: 400;">${message}</p>`)

                            tab_name.addEventListener('click', () => {
                                console.log("TAB CLICKED " + this.getVersion());
                                buildDisplayer.use(this)
                            })
                        }

                        getHTMLElements() {
                            return {
                                tab: document.getElementById('tab-' + this.getVersion()),
                                tab_name: document.getElementsByClassName('tab-name-' + this.getVersion())[0]
                            }
                        }
                    }

                    class BuildDisplayer {
                        constructor() {
                            this.current_tab = undefined
                            this.loaded_tabs = new Set()

                            this.displayLoading()
                        }

                        isLoaded(version) {
                            return this.loaded_tabs.has(version)
                        }

                        use(tab) {
                            if (this.current_tab === tab) {
                                console.log('Why are you trying to display the same tab?!')
                                return
                            }
                            console.log(`Displaying tab ${tab.getVersion()}`)
                            this.current_tab = tab
                            /*if(!this.isLoaded(tab.getVersion())) {
                                this.getLineElement().insertAdjacentHTML('afterbegin',
                                    `
                            <div class="pricing-column highlight" >
                                <h3>Build number
                                    <span class="rainbowgradient highlight-reason">DOWNLOAD BUILD</span>
                                </h3>
                                <div class="pricing-column-content">
                                    <p style="font-size: 55%; color: #c9c9c9; margin-bottom: -30px;">Enter build number below</p>
                                    <br>
                                    <div>
                                    <input type="text" name="version" minlength="1" maxlength="6" size="10" style="color: white; background: #111c24; height: 35px; border-radius: 8px; margin-top: -18px;">
                                    <a style="background: #111c24; padding: 6px; border-radius: 8px; cursor: pointer; color: white; margin-left: 10px; right: 30%; top: 39%;">Confirm</a>
                                    </div>
                                    <br>
                                    <p class="interval" style="font-size: 82%;letter-spacing: 0.021em;font-weight: 400;">Forge: undef</p>
                                    <br>
                                    <a style="margin-bottom: 20px;" class="nectar-button large regular m-extra-color-gradient-2" href="#" data-color-override="false" data-hover-color-override="false" data-hover-text-color-override="#fff">
                                        <span key="download">Download</span>
                                    </a>
                                    <div style="width: 100%; background: #111c24; height: 35px; position: absolute; bottom: 0; right: 0; border-radius: 5px;">
                                        <a style="color: white; cursor: pointer; font-size: 67%; position: absolute; left: 15px; bottom: 2px;">MD5: unknown</a>
                                        <a href="https://github.com/MohistMC/Mohist/commit/unknown" style="position: absolute; right: 10px; bottom: -10px;">
                                           <img src="https://icon-library.com/images/github-icon-white/github-icon-white-6.jpg" alt="View on GitHub" style="width: 25px; height: 25px;">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            `)
                            }*/
                            this.loaded_tabs.add(tab.getVersion())
                            buildDisplayer.trigger()
                        }

                        async trigger() {
                            if (!this.current_tab) return
                            this.hideLoading()
                            const lastFetchedBuildNumber = await this.current_tab.getLastFetchedBuild()
                            const line_element = this.getLineElement()
                            const availableSpace = 3 - line_element.children.length

                            let i = 1
                            let b = -1
                            while (i <= availableSpace) {
                                b++
                                const buildNumber = lastFetchedBuildNumber - b
                                if (buildNumber === 0) break
                                const buildInfos = await getBuildInfos(this.current_tab.getVersion(), buildNumber)
                                if (buildInfos.status === "FAILED")
                                    continue
                                line_element.insertAdjacentHTML('beforeend', this.getBuildHTML(buildInfos.number, buildInfos.name, buildInfos.md5, buildInfos.url, buildInfos.fullsha, buildInfos.forge_version, await this.current_tab.getLastBuildNumber() === buildNumber))
                                this.current_tab.setLastFetchedBuild(buildNumber - 1)
                                i++
                            }

                            this.displayLoading()
                        }

                        getLineElement() {
                            for (const line_html of this.current_tab.getHTMLElements().tab.getElementsByClassName('line-html')) {
                                if (line_html.children.length === 3) continue
                                return line_html
                            }
                            return this.insertNewLine()
                        }

                        insertNewLine() {
                            this.current_tab.getHTMLElements().tab.insertAdjacentHTML('beforeend', `<div class="row pricing-table three-cols line-html" data-style="flat-alternative"></div>`)
                            return this.current_tab.getHTMLElements().tab.getElementsByClassName('line-html')[this.current_tab.getHTMLElements().tab.getElementsByClassName('line-html').length - 1]
                        }

                        getBuildHTML(number, jar_name, md5, url, commit_sha, forge_version, latest) {
                            return `
                            <div class="pricing-column highlight extra-color-1">
                                <h3 style="margin-top: -15px;">#${number}
                                    <span class="rainbowgradient highlight-reason">${latest ? "LATEST" : "SUCCESS"}</span>
                                </h3>

                                <div class="pricing-column-content">
                                    <span class="interval" style="margin-bottom: -17px;">${jar_name}</span><br>
                                    <p class="interval" style="font-size: 82%;letter-spacing: 0.021em;font-weight: 400;">Forge: ${forge_version}</p>
                                    <br>
                                    <a style="margin-bottom: 20px;" class="nectar-button large regular m-extra-color-gradient-2" href="https://adfoc.us/serve/sitelinks/?id=765928&url=${url}" data-color-override="false" data-hover-color-override="false" data-hover-text-color-override="#fff">
                                        <span key="download">Download</span>
                                    </a>
                                    <div style="width: 100%; background: #111c24; height: 35px; position: absolute; bottom: 0; right: 0; border-radius: 5px;">
                                        <a style="color: white; cursor: pointer; font-size: 67%; position: absolute; left: 15px; bottom: 2px;">MD5: ${md5}</a>
                                        <a href="${url}" style="position: absolute; right: 10px; bottom: 30px;">
                                           <img src="/resources/images/downloadIcon.webp" alt="Mirror download" style="width: 25px; height: 25px;">
                                        </a>
                                        <a href="https://github.com/MohistMC/Mohist/commit/${commit_sha}" style="position: absolute; right: 10px; bottom: -10px;">
                                           <img src="https://icon-library.com/images/github-icon-white/github-icon-white-6.jpg" alt="View on GitHub" style="width: 25px; height: 25px;">
                                        </a>
                                    </div>
                                </div>
                            </div>
                            `
                        }

                        displayLoading() {
                            document.getElementById("loading").style.visibility = "visible"
                        }

                        hideLoading() {
                            document.getElementById("loading").style.visibility = "hidden"
                        }
                    }

                    const buildDisplayer = new BuildDisplayer()

                    const tabManager = new TabManager()
                    tabManager.init()

                    window.addEventListener('scroll', function () {
                        const element = document.querySelector('#loading');
                        const position = element.getBoundingClientRect();

                        if (document.getElementById("loading").style.visibility === "visible") {
                            if (position.top < window.innerHeight && position.bottom >= 0) {
                                buildDisplayer.trigger()
                            }
                        }
                    });
                </script>
